<?xml version="1.0" encoding="ISO-8859-1"?>
<mongodb:Database xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:mongodb="som.umltonosql.metamodel.documentdb">
  <collections name="Order">
    <documents>
      <fields key="reference">
        <type xsi:type="mongodb:SimpleType" name="String"/>
      </fields>
      <fields key="shipmentDate">
        <type xsi:type="mongodb:SimpleType" name="Date"/>
      </fields>
      <fields key="deliveryDate">
        <type xsi:type="mongodb:SimpleType" name="Date"/>
      </fields>
      <fields key="paid">
        <type xsi:type="mongodb:SimpleType" name="Boolean"/>
      </fields>
      <fields key="_id">
        <type xsi:type="mongodb:SimpleType" name="String"/>
      </fields>
      <fields key="client">
        <type xsi:type="mongodb:UmlToNoSQLIDReference" name="Client"/>
      </fields>
    </documents>
  </collections>
  <collections name="Product">
    <documents>
      <fields key="name">
        <type xsi:type="mongodb:SimpleType" name="String"/>
      </fields>
      <fields key="price">
        <type xsi:type="mongodb:SimpleType" name="Integer"/>
      </fields>
      <fields key="description">
        <type xsi:type="mongodb:SimpleType" name="String"/>
      </fields>
      <fields key="_id">
        <type xsi:type="mongodb:SimpleType" name="String"/>
      </fields>
    </documents>
  </collections>
  <collections name="OrderLine">
    <documents>
      <fields key="_id">
        <type xsi:type="mongodb:SimpleType" name="String"/>
      </fields>
    </documents>
  </collections>
</mongodb:Database>
